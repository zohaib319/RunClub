<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff">


    <TextView
        android:text="@string/import_fb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="20dp"

        android:layout_marginTop="20dp"
        android:id="@+id/textView14"
        android:textColor="#000000"
        android:textSize="15sp"/>


    <View
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:background="#bababc"
        android:layout_below="@+id/textView14"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="15dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:text="@string/import_button"
        android:layout_width="wrap_content"
        android:layout_height="30dp"

        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="23dp"
        android:layout_marginEnd="23dp"
        android:id="@+id/button_import"
        android:textColor="#ffffff"
        android:background="#0ceb16"
        android:layout_alignTop="@+id/textView14"
        android:layout_marginTop="-7dp"/>


    <View
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:background="#bababc"
        android:layout_below="@+id/button_import"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="65dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/view" />

    <TextView
        android:text="@string/import_contacts"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView15"
        android:textColor="#000000"
        android:textSize="15sp"
        android:layout_alignBottom="@+id/view"
        android:layout_alignRight="@+id/textView14"
        android:layout_alignEnd="@+id/textView14"
        android:layout_marginBottom="18dp" />

    <Button
        android:text="@string/import_button"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:id="@+id/button_import2"
        android:textColor="#ffffff"
        android:background="#0ceb16"
        android:layout_marginRight="23dp"
        android:layout_marginEnd="23dp"
        android:layout_marginTop="-7dp"
        android:layout_alignTop="@+id/textView15"
        android:layout_alignLeft="@+id/button_import"
        android:layout_alignStart="@+id/button_import" />

    <TextView
        android:text="@string/import_text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView16"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="17dp"
        android:id="@+id/textView17"
        android:textSize="15sp"
        android:textColor="#000000"
        />

    <TextView
        android:text="@string/import_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="81dp"
        android:id="@+id/textView16"
        android:textColor="#000000"
        android:textSize="15sp"
        android:layout_below="@+id/view"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="@string/import_text3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView17"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="24dp"
        android:id="@+id/textView18"
        android:textSize="14sp"/>

    <Button
        android:text="@string/addfriends_button"
        android:layout_width="320dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="81dp"
        android:id="@+id/button7"

        android:background="#000000"
        android:textColor="#ffffff"
        android:textSize="15sp"
        android:layout_below="@+id/textView18"
        android:layout_alignLeft="@+id/textView14"
        android:layout_alignStart="@+id/textView14" />


</RelativeLayout>